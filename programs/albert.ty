func main() {

   var x: Int
   x = 0

   var message: String
   message = ""

   while (x < 10) {

        if (x < 10 && x > 7 ) {
            message = "x is almost 10"
        } else if (x == 5) {
            message = "x is == to 5"
        } else {
            message = "x is growing up so fast :("
        }
        print(message)
        x = x + 1
   }


}
